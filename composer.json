{
  "name": "itplusx/headless-gridelements",
  "type": "typo3-cms-extension",
  "description": "Grid Elements json output for EXT:headless",
  "keywords": [
    "TYPO3",
    "TYPO3 CMS",
    "extension",
    "gridelements",
    "headless"
  ],
  "homepage": "https://itplusx.de",
  "support": {
    "email": "typo3@itplusx.de",
    "issues": "https://github.com/itplusx/headless_gridelements/issues"
  },
  "authors": [
    {
      "name": "Ram√≥n Schlosser",
      "email": "schlosser@itplusx.de",
      "role": "Developer",
      "homepage": "https://itplusx.de"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "typo3/cms-core": "^12.4",
    "friendsoftypo3/headless": "^4.0",
    "gridelementsteam/gridelements": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "ITplusX\\HeadlessGridelements\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ITplusX\\HeadlessGridelements\\Tests\\": "Tests"
    }
  },
  "replace": {
    "typo3-ter/headless_gridelements": "self.version"
  },
  "extra": {
    "branch-alias": {
      "dev-main": "4.0.x-dev"
    },
    "typo3/cms": {
      "extension-key": "headless_gridelements"
    }
  }
}
